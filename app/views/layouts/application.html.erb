<!DOCTYPE html>
<html>
  <head>
    <title>Connecti</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
<div>
    <div>
      <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQVubMqKgrgkuc0anKdj7SIYEWNuoNQLyeXUnbotkJwFdsp_hP2" width="60">


    </div>
    <div>
      <% if user_signed_in? %>
      <p><%= current_user.email %></p>
      <p><%= link_to "Sign Out", destroy_user_session_path, method: :delete %> </p>
      <% else %>
      <p><%= link_to "Sign In", new_user_session_path %></p>
      <p> <%= link_to "Sign Up", new_user_registration_path %></p>
      <% end %>
    </div>

</div>


    <%= yield %>
  </body>
</html>
